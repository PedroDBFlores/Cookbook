{
  "include": [
    "./src"
  ],
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/",
    "strict": true,
    "incremental": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "ES2020",
    "target": "ES2020",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "useUnknownInCatchVariables": false,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "noEmit": true,
    "paths": {
      "@webapp/components/*": [
        "./components"
      ],
      "@webapp/features/*": [
        "./features"
      ],
      "@webapp/model/*": [
        "./model"
      ],
      "@webapp/services/*": [
        "./services"
      ],
      "@webapp/utils/*": [
        "./utils"
      ]
    }
  }
}